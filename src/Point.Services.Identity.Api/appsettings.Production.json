{
	"Serilog": {
		"Using": [
			"Serilog.Sinks.Console"
			//"Serilog.Sinks.Seq"
		],
		"MinimumLevel": {
			"Default": "Warning",
			"Override": {
				"Microsoft": "Warning",
				"System": "Warning"
			}
		},
		"WriteTo": [
			{
				"Name": "Console"
			},
			{
				"Name": "Seq",
				"Args": {
					"serverUrl": "http://localhost:5341",
					"apiKey": "none"
				}
			}
		],
		"Enrich": [
			"FromLogContext"
		],
		"Properties": {
			"ApplicationName": "Identity"
		}
	}
}